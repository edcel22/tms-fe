            poppins_bold(14px, true)
            color: $dark_primary
            margin-bottom: 10px
            &.inline
                display: flex
                flex-flow: row wrap
                align-items: center
                justify-content: space-between
        .box_group
            padding: 10px
            box-shadow: 0 0 30px rgba(0, 0, 0, 0.1)
        .box_group_inline
            display: flex
            flex-flow: row wrap
            align-items: center
            justify-content: flex-start
.box_inline
    display: flex
    flex-flow: row wrap
    justify-content: space-between
    .box
        flex: 0 0 calc(50% - 15px)

/**
 * Loader */
.loader
    position: fixed
    top: 0
    left: 0
    bottom: 0
    right: 0
    z-index: 99955
    background-color: rgba(0, 132, 176, 0.2)
    .wrapper
        position: absolute
        top: 50%
        left: 0
        right: 0
        text-align: center
        margin: 0 auto
        width: 250px
        transform: translateY(-50%)
        .icon
            margin-bottom: 10px
        .dots
            display: inline-block
            position: relative
            width: 80px
            height: 80px
            div
                animation: rotate 1.2s cubic-bezier(0.5, 0, 0.5, 1) infinite
                transform-origin: 40px 40px
                &::after
                    content: " "
                    display: block
                    position: absolute
                    width: 7px
                    height: 7px
                    border-radius: 50%
                    background-color: $primary
                    margin: -4px 0 0 -4px
                &:nth-child(1)
                    animation-delay: -0.036s
                    &::after
                        top: 63px
                        left: 63px
                &:nth-child(2)
                    animation-delay: -0.072s
                    &::after
                        top: 68px
                        left: 56px
                &:nth-child(3)
                    animation-delay: -0.108s
                    &::after
                        top: 71px
                        left: 48px
                &:nth-child(4)
                    animation-delay: -0.144s
                    &::after
                        top: 72px
                        left: 40px
                &:nth-child(5)
                    animation-delay: -0.18s
                    &::after
                        top: 71px
                        left: 32px
                &:nth-child(6)
                    animation-delay: -0.216s
                    &::after
                        top: 68px
                        left: 24px
                &:nth-child(7)
                    animation-delay: -0.252s
                    &::after
                        top: 63px
                        left: 17px
                &:nth-child(8)
                    animation-delay: -0.288s
                    &::after
                        top: 56px
                        left: 12px

#dialog
    position: fixed
    left: 0
    z-index: 1024
    &.centered
        .wrapper
            .content
                top: 50%
                transform: translateY(-50%)
                animation: centered_slide 1s
    &.overlap
        z-index: 9999
    .wrapper
        position: fixed
        top: 0
        left: 0
        z-index: 1050
        width: 100%
        height: 100%
        outline: 0
        overflow-x: hidden
        overflow-y: auto
        .content
            position: relative
            z-index: 1041
            width: auto
            max-width: 600px
            border-radius: 5px
            margin: 0 auto
            background-color: $white
            box-shadow: 0 0 30px rgba(0, 0, 0, 0.3)
            transform: translateY(50px)
            animation: basic_slide 1s
            &.extended
                max-width: 900px
            &.secondary
                .close
                    background-color: $dark_primary
                .content_top
                    color: $dark_primary
            &.success
                .close
                    background-color: $success
                .content_top
                    color: $success
            &.cancel
                .close
                    background-color: $red
                .content_top
                    color: $red
            &.warning
                .close
                    background-color: $warning
                .content_top
                    color: $warning
            &.info
                .close
                    background-color: $red
                .content_top
                    color: $blue
            &.dark
                .close
                    background-color: $gray
                .content_top
                    color: $gray
            .close
                position: absolute
                top: -3px
                right: -3px
                display: inline-block
                padding: 5px 10px
                opacity: 1
                border-radius: 5px
                poppins_regular(20px, false)
                background-color: $primary
                color: $white
                box-shadow: 0 0 10px rgba(0, 0, 0, 0.3)
                z-index: 2
                line-height: 1
                transition: .3s ease-in-out
                &:hover
                    top: -6px
                    right: -6px
                    transition: .3s ease-in-out
            .content_top
                border-radius: 5px 5px 0 0
                background-color: rgba(0, 0, 0, 0.05)
                padding: 10px 20px
                poppins_medium(20px, false)
                color: $primary
            .content_middle
                padding: 10px 20px
                poppins_light(16px, false)
                color: $black
                .disclaimer
                    .violator
                        poppins_light(16px, false)
                        color: $black
            .content_bottom
                border-top: 2px solid rgba(0, 0, 0, 0.05)
                text-align: right
                padding: 10px 20px
            @media(max-width: 768px)
                width: 100%
                max-width: calc(100% - 20px)
                margin: 0 10px
    .backdrop
        position: fixed
        top: 0
        z-index: 1040
        width: 100vw
        height: 100vh
        background-color: rgba(0, 0, 0, 0.2)

/**
 * Forms */

/**
 * Switch */
#switch
    display: flex
    flex-flow: row wrap
    align-items: center
    &.disabled
        opacity: 0.5
        pointer-events: none
    &.toggled
        &.secondary
            label
                background-color: $dark_primary
                border: 2px solid $dark_primary
                transition: .3s ease-in-out
        &.success
            label
                background-color: $success
                border: 2px solid $success
                transition: .3s ease-in-out
        &.cancel
            label
                background-color: $red
                border: 2px solid $red
                transition: .3s ease-in-out
        &.warning
            label
                background-color: $warning
                border: 2px solid $warning
                transition: .3s ease-in-out
        &.info
            label
                background-color: $blue
                border: 2px solid $blue
                transition: .3s ease-in-out
        &.dark
            label
                background-color: $gray
                border: 2px solid $gray
                transition: .3s ease-in-out
        label
            background-color: $primary
            border: 2px solid $primary
            transition: .3s ease-in-out
            &::before
                left: calc(100% - 22px)
                transition: .3s ease-in-out
    label
        cursor: pointer
        position: relative
        display: inline-block
        width: 50px
        height: 25px
        background-color: $gray_v3
        border: 2px solid $gray_v3
        border-radius: 50px
        transition: .3s ease-in-out
        &::before
            content: ''
            position: absolute
            top: 0
            left: 0
            width: 22px
            height: 100%
            border-radius: 50%
            background-color: $white
            z-index: 1
            transition: .3s ease-in-out
    input
        position: absolute
        left: 0
        z-index: -1
        opacity: 0
        visibility: hidden
    span
        display: inline-block
        padding-left: 7.5px
        poppins_regular(14px, true)
        color: $gray
// multiple select
.custom_select
	position: relative
	&.not_allowed
		.selected_item
			cursor: not-allowed
	.selected_item
		font-family: 'Poppins-Regular'
		font-size: 16px
		position: relative
		padding: 10px 30px 10px 15px
		border: 1px solid #0084b0
		cursor: pointer
		border-radius: 2px
		&:after
			content: ''
			position: absolute
			top: 42%
			right: 10px
			border-left: 7px solid transparent
			border-right: 7px solid transparent
			border-top: 7px solid #0084b0
			transition: all 0.3s ease-in-out
	.select_dropdown
		position: absolute
		background: white
		border-radius: 2px
		box-shadow: 15px 15px 15px #0B5AAB26
		z-index: 2
		max-height: 300px
		overflow-y: auto
		overflow-x: hidden
		left: 0
		right: 0
		&::-webkit-scrollbar
			width: 10px
			background-color: transparent
		&::-webkit-scrollbar-track
			box-shadow: none
		&::-webkit-scrollbar-thumb
			background-color: #0b5aab
			border: 3px solid transparent
			background-clip: padding-box
			border-radius: 50px
		.dropdown_item
			padding: 15px
			opacity: 1
			.radio_container
				display: flex
				align-items: flex-start
				position: relative
				padding-left: 35px
				cursor: pointer
				font-weight: normal
				font-size: 14px
				font-family: 'Poppins-Regular'
				input
					position: absolute
					opacity: 0
					cursor: pointer
					border: 1px solid blue
				.checkmark
					position: absolute
					top: 0
					left: 0
					height: 19px
					width: 19px
					border-radius: 2px
					border: 1px solid #0084b0
					&.checked 
						border: 2px solid #0084b0
						&:after
							content: ""
							position: absolute
							left: 5px
							top: 1px
							width: 5px
							height: 10px
							border: solid #0084b0
							border-width: 0 2px 2px 0
							-webkit-transform: rotate(45deg)
							-ms-transform: rotate(45deg)
							transform: rotate(45deg)
.card
    display: flex
    background: $white
    border-radius: 8px
    box-shadow: 0 0 5px #4b556385
    flex-flow: row wrap
    justify-content: space-evenly
    margin: 0
    min-height: 70px
    width: 100%
    padding: 10px 40px 20px 10px

/**
 * Input */
.form_group
    #input
        display: block
        width: 100%

.fade
    animation: fade 1s

@keyframes fade
    0% {
        opacity: 0
    }
    100% {
        opacity: 1
    }

@keyframes rotate
    0% {
        transform: rotate(0deg)
    }
    100% {
        transform: rotate(360deg)
    }

@keyframes basic_slide
	from {
		opacity: 0
        transform: translateY(0)
    }
	to {
		opacity: 1
        transform: translateY(50px)
    }

@keyframes centered_slide
	from {
		opacity: 0
        transform: translateY(-100%)
    }
	to {
		opacity: 1
        transform: translateY(-50%)
    }

@keyframes right_slide
    from {
        opacity: 0
        transform: translateX(100px)
    }
    to {
        opacity: 1
        transform: translateX(0)
    }

@keyframes filter_slide
    from {
        opacity: 0
        transform: translateX(450px)
    }
    to {
        opacity: 1
        transform: translateX(0)
    }

.page-enter-active,
.page-leave-active
    transition: all .3s ease-in-out

.page-enter,
.page-leave-to
    opacity: 0
    transform: scale(1.05)

.fade-enter-active,
.fade-leave-active
    transition: all .3s ease-in-out

.fade-enter,
.fade-leave-to
    opacity: 0

.right_slide-enter-active,
.right_slide-leave-active
    transition: all .3s ease-in-out

.right_slide-enter,
.right_slide-leave-to
    opacity: 0
    transform: translateX(100px)

.slide-enter-active,
.slide-leave-active
    transition: all .2s ease-in-out

.slide-enter,
.slide-leave-to
    opacity: 0
    transform: translateY(-20px)
