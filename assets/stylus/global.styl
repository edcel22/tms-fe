/**
 * Avatar */
.avatar
    position: relative
    width: 75px
    height: 75px
    border-radius: 50%
    &.primary
        background-color: rgba(0, 132, 176, 0.2)
    &.secondary
        background-color: rgba(2, 5, 29, 0.2)
    &.success
        background-color: rgba(0, 160, 20, 0.2)
    &.cancel
        background-color: rgba(234, 84, 85, 0.2)
    &.warning
        background-color: rgba(255, 200, 0, 0.2)
    &.info
        background-color: rgba(67, 142, 255, 0.2)
    &.dark
        background-color: rgba(122, 122, 122, 0.2)
    .overlap
        position: absolute
        top: 50%
        left: 0
        right: 0
        text-align: center
        margin: 0 auto
        transform: translateY(-50%)

/**
 * Form */
.form
    position: relative
    &.flex
        flex: 1 0 auto
    .validate
        position: absolute
        top: calc(100% + 5px)
        background-color: $red
        color: $white
        poppins_regular(14px, false)
        z-index: 1
        padding: 5px 10px
        border-radius: 5px
        user-select: none
        right: 0
        &::before
            content: ""
            position: absolute
            bottom: calc(100% - 1px)
            width: 0
            height: 0
            border-left: 7px solid transparent
            border-right: 7px solid transparent
            border-bottom: 7px solid $red
            right: 17px
            z-index: 1
    .group_inline
        position: relative
        display: flex
        flex-flow: row wrap
        align-items: center
        justify-content: flex-start
        margin-bottom: 20px
        &.no_align
            align-items: unset
        &.between
            justify-content: space-between
        &.two
            justify-content: space-between
            .group
                width: 100%
                flex: 0 0 49.33%
                @media(max-width: 500px)
                    flex: 0 0 100%
        &.three
            justify-content: space-between
            @media(max-width: 500px)
                flex-flow: column wrap
                justify-content: flex-start
                align-items: flex-start
            .group
                flex: 1 0 calc(32.33% - 5px)
                @media(max-width: 500px)
                    width: 100%
                    flex: 1 0 100%
                &:nth-child(2)
                    margin: 0 10px
                    @media(max-width: 500px)
                        margin: 0 0 20px
        &.four
            justify-content: space-between
            .group
                flex: 0 0 24.33%
        .group
            flex: 0 0 auto
            margin-bottom: 0
            &.radios
                margin-right: 20px
            .checkbox
                margin-right: 20px
            &:last-child
                &.radios
                    margin-right: 0
                .checkbox
                    margin-right: 0
    .call_to_action
        poppins_regular(14px, false)
        color: $gray
        transition: .3s ease-in-out
        &:hover
            color: $secondary
            transition: .3s ease-in-out
    .disclaimer
        margin: 5px 0 0
        .violator
            poppins_light(14px, false)
            font-style: italic
            color: $black
            b
                poppins_bold(14px, false)
                color: $secondary
                font-style: italic
    .preview_image
        display: flex
        flex-flow: row wrap
        align-items: flex-start
        margin: 30px 0 0
        &.scrollable
            padding: 20px 0
            height: 100%
            max-height: 300px
            overflow-y: auto
            overflow-x: hidden
        &.one
            .preview
                flex: 0 0 calc(100% - 20px)
        &.two
            .preview
                flex: 0 0 calc(50% - 20px)
        &.three
            .preview
                flex: 0 0 calc(33.33% - 20px)
        &.four
            .preview
                flex: 0 0 calc(25% - 20px)
        .preview
            position: relative
            z-index: 0
            flex: 0 0 calc((100% / 6) - 20px)
            margin: 0 10px 20px
            animation: slide 1s
            &.whole
                flex: 0 0 100%
                margin: 0
            &.big
                .item
                    padding: 40px 20px
            img
                width: 100%
                height: 100px
                object-fit: cover
            .item
                position: relative
                width: 100%
                padding: 20px
                background-image: linear-gradient(45deg, $dark_primary, $primary)
                span
                    position: absolute
                    top: 50%
                    left: 0
