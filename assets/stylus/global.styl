@import 'fonts'

$primary = #3e8ef7
$secondary = #f15928
$blue_gray = #D0E5EC
$blue = #20B2E7
$dark = #4A5061
$dark_primary = #020F1D
$black = #111
$white = #FFF
$blue = #438EFF
$red = #EA5455
$gray = #7A7A7A
$green = #66cc99
$gray_v2 = #F6F6F6
$gray_v3 = #C3C3C3
$gray_v4 = #E2E2E2
$success = #5cbf69
$warning = #ffc800

.text-success
    color: #66cc99
.text-danger
    color: $red

.flex-g
    flex-grow: 1
.d-flex
    display: flex

.m-lr
    margin: 0 5px
html
    -webkit-text-size-adjust: none
    -webkit-font-smoothing: subpixel-antialiased
    backface-visibility: hidden
    poppins_regular(18px, false)

body
    &::-webkit-scrollbar
        width: 20px
        background-color: $gray_v2
    &::-webkit-scrollbar-track
        box-shadow: none
    &::-webkit-scrollbar-thumb
        background-color: $gray
        border: 6px solid rgba(0, 0, 0, 0)
        background-clip: padding-box
        border-radius: 50px

::-webkit-scrollbar-track
	-webkit-box-shadow: inset 0 0 6px rgba(0,0,0,0.1)
	background-color: $gray_v2
	border-radius: 10px
::-webkit-scrollbar
	width: 5px
	height: 10px
	background-color: $gray_v2
::-webkit-scrollbar-thumb
	border-radius: 10px
	background-color: $gray

body
    overflow-x: hidden
    background-color: $gray_v2
    &.cms
    &.no_click
        pointer-events: none
    &.no_scroll
        overflow: hidden

*,
*:before,
*:after
    box-sizing: border-box
    margin: 0

button
    outline: none

input,
input:before,
input:after,
textarea,
textarea:before,
textarea:after,
select,
select:before,
select:after
    user-select: initial
a
    text-decoration: none

input:focus
    outline: none


.input
    &.number
        padding: 5px
        border: 2px solid #0000001c
        &.no_incredecre
            &::-webkit-inner-spin-button,
            &::-webkit-outer-spin-button
                -webkit-appearance: none
                -moz-appearance: none
                appearance: none
                margin: 0 
                visibility: hidden
h1,
h2,
h3,
h4,
h5
    font-weight: normal

h1
    font-family: 'Poppins-Medium'
    font-size: 22px
h2
    font-family: 'Poppins-Medium'
    font-size: 18px

img
    height: auto
    vertical-align: middle

table
    width: 100%
    border-collapse: collapse

.hidden
    display: none

.o_hidden
    position: absolute
    opacity: 0
    visibility: hidden
    z-index: -1

.none
    pointer-events: none

@page
    size: landscape
    margin: 0cm

@media print
    body
        margin: 0.75cm
        -webkit-print-color-adjust: exact

svg,
span
    vertical-align: middle

.pointer
    cursor: pointer

.mb
    margin-bottom: 30px
    &.seventy
        margin-bottom: 70px
    &.twenty
        margin-bottom: 20px !important
.mr
    margin-right: 30px
    &.ten
        margin-right: 10px
    &.twenty
        margin-right: 20px !important
.ml
    margin-left: 30px
    &.ten
        margin-left: 10px
    &.twenty
        margin-left: 20px !important
.nmb
    margin-bottom: 0 !important
.nmr
    margin-right: 0 !important

.note-editor
    &.note-frame
        border: 1px solid $primary
        border-radius: 0
        .note-toolbar
            background-color: $white
        .note-editing-area
            .note-editable
                poppins_regular(16px, false)
                padding: 15px

.blue
    color: $blue !important
.green
    color: #30D988 !important
.red
    color: #F15928 !important

.CodeMirror
    z-index: 0 !important

.scrollable_content
    height: 100%
    max-height: 300px
    overflow-y: auto
    overflow-x: hidden


a
 color: black
 &:hover,
 &:active,
 &:focus
    color: black

/**
 * Root */
.task_management_system
    position: relative
    &.no_scroll
        overflow: hidden
    .swallower
        position: sticky
        top: 0
        z-index: 1
        height: 85px
        width: 100%
        pointer-events: none
        background: -webkit-gradient(linear,left top,left bottom,color-stop(41%,#fff),color-stop(95%,hsla(0,0%,100%,.11)),to(hsla(0,0%,100%,0)))
    &.full
        #header
            max-width: calc(100% - 130px)
            transition: .3s ease-in-out
        #footer
            max-width: calc(100% - 70px)
            transition: .3s ease-in-out
        .page
            margin-left: 70px
            transition: .3s ease-in-out
    .page
        position: relative
        padding: 72px 30px 60px
        margin-left: 260px
        transition: .3s ease-in-out
        @media(max-width: 768px)
            margin: 0
            padding: 70px 10px 0 10px
        @media(max-width: 500px)
            padding: 72px 10px 0 10px
        &.error
            .wrapper
                display: flex
                flex-flow: column wrap
                align-items: center
                justify-content: center
                height: calc(100vh - 205px)
                .title
                    margin-top: 10px
                    poppins_bold(26px, true)
                .subtitle
                    margin: 10px 0 20px
                    poppins_regular(18px, false)

/**
 * Footer */
#footer
    position: fixed
    bottom: 0
    z-index: 2
    right: 0
    width: 100%
    max-width: calc(100% - 260px)
    display: flex
    flex-flow: row wrap
    align-items: center
    padding: 10px 30px
    background-image: linear-gradient(45deg, $dark_primary, $primary)
    justify-content: space-between
    transition: .3s ease-in-out
    .label
        color: $white
        poppins_regular(14px, false)

/**
 * Breadcrumb */
#breadcrumb
    &.left
        .list
            justify-content: flex-start
    &.right
        .list
            justify-content: flex-end
    &.center
        .list
            justify-content: center
    .list
        list-style: none
        padding: 0
        margin: 0
        display: flex
        flex-flow: row wrap
        align-items: center
        justify-content: flex-start
        .item
            display: flex
            flex-flow: row wrap
            align-items: center
            .link
                color: $black
                poppins_regular(14px, true)
                &.nuxt-link-active
                    color: $primary
                &.nuxt-link-exact-active
                    color: $black
                svg
                    display: block
            .separator
                color: $gray_v3
                padding: 0 5px
                poppins_regular(24px, false)
// Image Uplaoder
.edit_thumbnail
    diplay: none
.image_preview
    display: flex
    flex-direction: column
    img
        height: 400px
        object-fit: contain
        object-position: center
        background: #80808029

/**
 * Alert */
#alert
    &.primary
        background-color: rgba(0, 132, 176, 0.2)
        .wrapper
            p
                color: $primary
    &.secondary
        background-color: rgba(2, 5, 29, 0.2)
        .wrapper
            p
                color: $dark_primary
    &.success
        background-color: rgba(0, 160, 20, 0.2)
        .wrapper
            p
                color: $success
    &.error
        background-color: rgba(234, 84, 85, 0.2)
        .wrapper
            p
                color: $red
    &.warning
        background-color: rgba(255, 200, 0, 0.2)
        .wrapper
            p
                color: $warning
    &.info
        background-color: rgba(67, 142, 255, 0.2)
        .wrapper
            p
                color: $blue
    &.dark
        background-color: rgba(122, 122, 122, 0.2)
        .wrapper
            p
                color: $gray
    .wrapper
        padding: 10px
        border-radius: 5px
        p
            poppins_medium(16px, false)

/**
 * Toast */
#toast_wrapper
    position: fixed
    top: 0
    right: 0
    width: 400px
    height: 100vh
    z-index: 4
    background-color: transparent
    padding: 20px
    list-style: none
    overflow-y: auto
    overflow-x: hidden
    &::-webkit-scrollbar-track
    	-webkit-box-shadow: inset 0 0 6px rgba(0,0,0,0.1)
    	background-color: $gray_v2
    	border-radius: 10px
    &::-webkit-scrollbar
    	width: 5px
    	height: 10px
    	background-color: $gray_v2
    &::-webkit-scrollbar-thumb
    	border-radius: 10px
    	background-color: $gray
    #toast
        position: relative
        right: 0
        margin-bottom: 20px
        transform: translateX(0px)
        animation: right_slide 1s
        .close
            position: absolute
            top: -3px
            right: -3px
            display: inline-block
            padding: 5px 10px
            opacity: 1
            border-radius: 5px
            poppins_regular(20px, false)
            background-color: $primary
            color: $white
            box-shadow: 0 0 10px rgba(0, 0, 0, 0.3)
            z-index: 2
            transition: .3s ease-in-out
            &:hover
                top: -6px
                right: -6px
                transition: .3s ease-in-out
        .wrapper
            background-color: $white
            border-left: 5px solid $primary
            box-shadow: 0 0 30px rgba(0, 0, 0, 0.2)
            padding: 20px
            p
                color: $primary
                poppins_medium(18px, false)
        &.secondary
            .close
                background-color: $dark_primary
            .wrapper
                border-left: 5px solid $dark_primary
                p
                    color: $dark_primary
        &.success
            .close
                background-color: $success
            .wrapper
                border-left: 5px solid $success
                p
                    color: $success
        &.cancel
            .close
                background-color: $red
            .wrapper
                border-left: 5px solid $red
                p
                    color: $red
        &.warning
            .close
                background-color: $warning
            .wrapper
                border-left: 5px solid $warning
                p
                    color: $warning
        &.info
            .close
                background-color: $blue
            .wrapper
                border-left: 5px solid $blue
                p
                    color: $blue
        &.dark
            .close
                background-color: $gray
            .wrapper
                border-left: 5px solid $gray
                p
                    color: $gray
        &.last-child
            margin-bottom: 0

/**
 * Tabs */
#tabs
    .parent
        ul
            list-style: none
            padding: 0
            margin: 0
            display: flex
            flex-flow: row wrap
            align-items: flex-start
            justify-content: flex-start
            li
                padding: 15px 20px
                poppins_bold(14px, true)
                color: $gray
                border-bottom: 2px solid $gray
                background-color: $white
                transition: .3s ease-in-out
                &.active
                    color: $white
                    border-bottom: 2px solid $dark_primary
                    background-color: $primary
                    transition: .3s ease-in-out
                    a
                        color: $white
                &:hover
                    color: #626060
                    border-bottom: 2px solid #665e5e
                    background-color: #0000000d
                    transition: .3s ease-in-out
                    &.active
                        color: $white
                        border-bottom: 2px solid $dark_primary
                        background-color: $primary
                        transition: .3s ease-in-out
                a
                    color: #438eff
.tab_contents
    margin-top: 20px

/**
 * Collapse */
.collapse
    &.toggled
        .item_top
            &.top_box
                padding-bottom: 20px
                transition: .3s ease-in-out
            .arrow
                top: calc(50% - 10px)
                transform: translateY(-50%) rotateX(180deg)
                transition: .3s ease-in-out
        .item_bottom
            height: 100%
            opacity: 1
            visibility: visible
            transition: .3s ease-in-out
    .item_top
        position: relative
        &.top_box
            padding-bottom: 0
            transition: .3s ease-in-out
        .arrow
            content: ''
            position: absolute
            top: 50%
            right: 0
            border-left: 10px solid transparent
            border-right: 10px solid transparent
            border-top: 10px solid $dark_primary
            transform: translateY(-50%) rotateX(0deg)
            transition: .3s ease-in-out
    .item_bottom
        height: 0px
        opacity: 0
        visibility: hidden
        transition: .3s ease-in-out

/**
 * Buttons */
.primary_button
    display: inline-block
    poppins_medium(14px, true)
    color: $white
    padding: 10px 15px
    border: 1px solid #438eff
    background-color: #438eff
    border-radius: 10px
    box-shadow: 0 0 20px transparent
    text-align: center
    transform: translateY(0)
    transition: .3s ease-in-out
    cursor: pointer
    &.back
        display: flex
        align-items: center
        width: 94px
    &.rounded
        border-radius: 50px
    &.rectangle
        border-radius: 0px
    &.outline
        color: #438eff
        background-color: transparent
    &.flat
        color: #438eff
        border: none
        background-color: transparent
    &:hover
        box-shadow: 0 0 20px #939393
        transform: translateY(-5px)
        color: white !important
        transition: .3s ease-in-out
        &.outline
            color: $white
            background-color: #438eff
            transition: .3s ease-in-out
        &.flat
            box-shadow: none
            background-color: rgba(0, 132, 176, 0.2)
            transition: .3s ease-in-out
    svg
